<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.vo.RatingEntity" table="rating" schema="movielens">
        <id name="id" column="Id">
            <generator class="native"/>
        </id>
        <property name="rating" column="Rating"/>
        <property name="time" column="Time"/>
        <many-to-one name="movie" column="MovieId" class="org.vo.MovieEntity" cascade="all"/>
        <many-to-one name="user" column="UserId" class="org.vo.UserEntity" cascade="all"/>
    </class>
</hibernate-mapping>